<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>TeamTwo</display-name>
  <servlet>
    <servlet-name>userLoginServlet</servlet-name>
    <servlet-class>com.revature.servlets.UserLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userLoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>attendanceServlet</servlet-name>
    <servlet-class>com.revature.servlets.AttendanceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>attendanceServlet</servlet-name>
    <url-pattern>/student</url-pattern>
  </servlet-mapping>
</web-app>